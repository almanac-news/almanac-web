{
  "name": "almanac-web",
  "version": "0.1.0",
  "description": "Your one source of truth for financial news and data.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint ./src/**/*.js",
    "lint:tests": "eslint ./tests/**/*.spec.js",
    "compile": "webpack",
    "start": "node --harmony bin/server",
    "dev": "NODE_ENV=development npm start",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "node ./node_modules/karma/bin/karma start",
    "test:dev": "npm run test -- --watch",
    "deploy": "npm run lint && npm run test && npm run compile",
    "validate": "npm ls"
  },
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/almanac-web/almanac-web.git"
  },
  "author": "David Tsai",
  "license": "MIT",
